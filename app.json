{
   "expo": {
      "name": "CarpicK",
      "slug": "CarpicK",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#F7F8F9"
      },
      "packagerOpts": {
         "config": "metro.config.js",
         "sourceExts": ["js", "jsx", "ts", "tsx", "svg"]
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true,
         "infoPlist": {
            "NSAppTransportSecurity": {
               "NSAllowsArbitraryLoads": true
            }
         },
         "bundleIdentifier": "com.expo.CarpicK",
         "config": {
            "googleMapsApiKey": "AIzaSyBgavHAKokKBeXt4zJ5y0cNYPMoc1zYFdY"
         }
      },
      "android": {
         "package": "com.expo.CarpicK",
         "config": {
            "googleMaps": {
               "apiKey": "AIzaSyBgavHAKokKBeXt4zJ5y0cNYPMoc1zYFdY"
            }
         },
         "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#F7F8F9"
         }
      },
      "web": {
         "favicon": "./assets/favicon.png"
      }
   }
}
